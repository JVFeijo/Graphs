function clone(obj) {
    if (null == obj || "object" != typeof obj) return obj;
    var copy = obj.constructor();
    for (var attr in obj) {
        if (obj.hasOwnProperty(attr)) copy[attr] = obj[attr];
    }
    return copy;
}

var obj1 = { a: [1, 2, 3], b: [4, 5, 6] };
var obj2 = clone(obj1);

console.log(obj1, obj2);
obj1.a[1] = 9;
console.log(obj1, obj2);
